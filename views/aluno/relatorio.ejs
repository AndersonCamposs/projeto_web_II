<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        table {
            border: 1px solid #000;
        }
    </style>
</head>
<body>
    <h1>Relatório de alunos</h1>
    <table>
        <thead>
            <tr>
                <th>Nome</th>
                <th>Curso</th>
                <th>Matrícula</th>
            </tr>
        </thead>
        <tbody>
            <% lista.forEach(aluno => { %>
                <tr>
                    <td><%= aluno.nome%></td>
                    <td><%= aluno.curso%></td>
                    <td><%= aluno.matricula%></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>